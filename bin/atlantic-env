#!/usr/bin/env python

import json
import sys


def _load(path):
    return json.load(open(path, 'r'))


config = _load(sys.argv[1])

trans = {
    "_id": "ATLANTIC_BUILD_ID",
    "build": "ATLANTIC_BUILD_SEQUENCE",
    "dsc": "ATLANTIC_DSC_LOCATION"
}

for t in trans:
    print "export %s=%s" % (trans[t], config[t])
